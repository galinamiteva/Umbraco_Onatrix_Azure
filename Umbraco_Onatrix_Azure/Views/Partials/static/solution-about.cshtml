@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@inject Umbraco.Cms.Web.Common.UmbracoHelper UmbracoHelper

@{
    var rootContents = UmbracoHelper.ContentAtRoot();
    var settings = rootContents?.FirstOrDefault(x => x.ContentType.Alias == "settingsPage") ?? null;
    var solutionAboutImg = settings?.Value<IPublishedContent>("solutionAboutImg")?.Url() ?? null;
    var solutionAboutHeadline = settings?.Value<string>("solutionAboutHeadline") ?? null;
    var solutionAboutText = settings?.Value<string>("solutionAboutText") ?? null;

 
}


<h4>@solutionAboutHeadline</h4>
<div class="image">
    <img src="@solutionAboutImg" alt="Image of people">
</div>
<p>@solutionAboutText</p>
   